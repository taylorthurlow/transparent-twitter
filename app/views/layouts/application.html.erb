<!DOCTYPE html>
<html>
<head>
  <title>Transparent Twitter</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav>
    <ul>
      <li><%= link_to 'home', root_url %></li>
      <li><%= link_to 'trainer', trainer_url %></li>
      <li><%= link_to 'tweets', tweets_url %></li>
    </ul>
  </nav>

  <div id="content">
    <%= yield %>
  </div>

</body>
</html>
